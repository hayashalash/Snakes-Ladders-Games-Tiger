               <image>
                  <Image url="@../img/icons/X.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="homeButton" layoutX="731.0" layoutY="63.0" mnemonicParsing="false" onAction="#goHome" style="-fx-background-color: transparent;">
